aws emr create-cluster \
    --name "Clean Review Data (to SequenceFiles) 1/3" \
    --instance-type m3.xlarge \
    --release-label emr-4.0.0 \
    --instance-count 4 \
    --log-uri s3://bdnc2/logs/spark_cleandata_sequencefiles_1_3 \
    --use-default-roles \
    --applications Name=Spark \
    --steps file://../emr_configs/spark_clean_review_data_sequencefile_step.json \
    --configurations file://../emr_configs/spark_config.json \
    --auto-terminate
